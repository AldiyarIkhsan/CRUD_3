{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/testing/all-data",
      "dest": "api/testing/all-data.ts",
      "methods": ["DELETE"]
    },
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
}
